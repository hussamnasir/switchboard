
	<tr style="background-color:<%= cycle("#E6F2FF","white") %>">
		<td><%= link_to(group.name,group) %></td>
		
    <td><%= group.created_at.strftime("%Y-%m-%d %H:%M") unless group.created_at.blank?%></td>
