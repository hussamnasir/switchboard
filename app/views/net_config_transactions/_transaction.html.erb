<tr style="background-color:<%= cycle("#E6F2FF","white") %>">
	<td><%= transaction.who %></td>
	<td><%= transaction.target %></td>
	<td><%= transaction.description %></td>
	<td><%= transaction.status %></td>
	<td><%= transaction.command %></td>
	<td><%= transaction.response %></td>
	<td><%= transaction.created_at.strftime("%Y-%m-%d %H:%M:%S") unless transaction.created_at.blank?%></td>
	<td><%= transaction.id %></td>
</tr>
